@{
    ViewData["Title"] = "Datos nuevo Empleado";

}

<div class = "row">
    <div class="col-md-5">
        <form method="post" enctype="multipart/form-data" asp-controller="Employe" asp-action="Index">
            <div class="form-group">
                <div class="cold-md-10">
                    <p>Name</p>
                    <input class="form-control" name="employes" type="text"/>
                </div>
            </div><div class="form-group">
                <div class="cold-md-10">
                    <p>Rut</p>
                    <input class="form-control" name="employes" type="text"/>
                </div>
            </div><div class="form-group">
                <div class="cold-md-10">
                    <p>Company</p>
                    <input class="form-control" name="employes" type="text"/>
                </div>
            </div><div class="form-group">
                <div class="cold-md-10">
                    <p>Img name</p>
                    <input class="form-control" name="employes" type="text"/>
                </div>
            </div>
            <div class="form-group">
                <div class="cold-md-10">
                    <p>Img File</p>
                    <input class="form-control" name="employes" type="file"/>
                </div>
            </div>
            <div class="form-group">
                <div class="cold-md-10">
                    <input class="btn btn-success" type="submit" value="Upload"/>
                </div>
            </div>
        </form>
    </div>
</div>